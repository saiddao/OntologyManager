{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": [
      "SoSs"
    ],
    "properties": {
      "SoSs": {
        "type": "array",
        "items": {
          "type": "object",
          "required": [
            "sosName",
            "description",
            "sosId",
            "devices"
          ],
          "properties": {
            "sosName": {
              "type": "string"
            },
            "description": {
              "type": "string"
            },
            "sosId": {
              "type": "string"
            },
            "devices": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "deviceName",
                  "description",
                  "deviceId",
                  "components"
                ],
                "properties": {
                  "deviceName": {
                    "type": "string"
                  },
                  "description": {
                    "type": "string"
                  },
                  "deviceId": {
                    "type": "string"
                  },
                  "components": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "required": [
                        "componentName",
                        "description",
                        "componentId",
                        "skills"
                      ],
                      "properties": {
                        "componentName": {
                          "type": "string"
                        },
                        "description": {
                          "type": "string"
                        },
                        "componentId": {
                          "type": "string"
                        },
                        "skills": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "required": [
                              "skillName",
                              "description",
                              "skillId",
                              "rules"
                            ],
                            "properties": {
                              "skillName": {
                                "type": "string"
                              },
                              "description": {
                                "type": "string"
                              },
                              "skillId": {
                                "type": "string"
                              },
                              "rules": {
                                "type": "array",
                                "items": {
                                  "type": "object",
                                  "required": [
                                    "ruleName",
                                    "description",
                                    "ruleId",
                                    "ruleType",
                                    "abstractrule",
                                    "welldefined"
                                  ],
                                  "properties": {
                                    "ruleName": {
                                      "type": "string"
                                    },
                                    "description": {
                                      "type": "string"
                                    },
                                    "ruleId": {
                                      "type": "string"
                                    },
                                    "ruleType": {
                                      "type": "string"
                                    },
                                    "abstractrule": {
                                      "type": "string"
                                    },
                                    "welldefined": {
                                      "type": "string"
                                    }
                                  }
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
  